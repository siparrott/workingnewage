{
  "name": "photography-crm-demo",
  "version": "1.0.1",
  "description": "Live demo of Photography CRM SaaS platform",
  "main": "minimal-server.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "engineStrict": false,
  "scripts": {
    "prebuild": "echo 'Preparing build environment...' && npm --version",
    "dev": "cross-env NODE_ENV=development DEMO_MODE=true tsx scripts/dev-start.ts",
    "dev:win": "set NODE_ENV=development&& set DEMO_MODE=true&& node .\\node_modules\\tsx\\dist\\cli.mjs scripts\\dev-start.ts",
    "start:server": "node .\\node_modules\\tsx\\dist\\cli.mjs server\\index.ts",
    "build": "npx vite build --mode production",
    "build:vercel": "npx vite build --mode production",
    "build:netlify": "npm ci --legacy-peer-deps --include=dev && npx vite build --mode production",
    "build:debug": "echo 'Debug build starting...' && npm list vite tailwindcss && npx vite build --mode production --logLevel info",
    "build:clean": "echo 'Clean skipped'",
    "build:client": "npx vite build --mode production",
    "build:server": "echo 'Server build skipped - using tsx for production'",
    "build:server:prod": "tsc --project tsconfig.server.json",
    "build:full": "npm run build:client && npm run build:server:prod",
    "validate:deps": "npm list vite tailwindcss autoprefixer postcss",
    "start": "node full-server.js",
    "start:prod": "cross-env NODE_ENV=production node start.mjs",
    "heroku-postbuild": "npm run build",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:init": "tsx scripts/init-database.ts",
    "db:init:galleries": "tsx scripts/init-galleries.ts",
    "db:setup": "npm run db:push && npm run db:init",
    "create:admin": "tsx scripts/create-admin-user.ts",
    "demo:setup": "node scripts/setup-demo-data.js",
    "demo:reset": "node scripts/reset-demo-data.js",
    "diagnose:crm": "tsx scripts/diagnose-crm.ts",
    "deploy:start": "cross-env NODE_ENV=production tsx server/index.ts",
    "import:dry": "cross-env BASE_URL=http://127.0.0.1:3000 ICS_URL=https://calendar.google.com/calendar/ical/newagefotografen%40gmail.com/public/basic.ics node scripts/reimport-ics.js --dry-run",
    "import:full": "cross-env BASE_URL=http://127.0.0.1:3000 ICS_URL=https://calendar.google.com/calendar/ical/newagefotografen%40gmail.com/public/basic.ics node scripts/reimport-ics.js",
    "import:prod:dry": "cross-env BASE_URL=https://www.newagefotografie.com ICS_URL=https://calendar.google.com/calendar/ical/newagefotografen%40gmail.com/public/basic.ics node scripts/reimport-ics.js --dry-run",
    "import:prod:full": "cross-env BASE_URL=https://www.newagefotografie.com ICS_URL=https://calendar.google.com/calendar/ical/newagefotografen%40gmail.com/public/basic.ics node scripts/reimport-ics.js",
    "cleanup:prod:dry": "cross-env BASE_URL=https://www.newagefotografie.com THRESHOLD=20 LIMIT=200 node scripts/cleanup-stacked.js",
    "cleanup:prod:exec": "cross-env BASE_URL=https://www.newagefotografie.com THRESHOLD=20 LIMIT=200 node scripts/cleanup-stacked.js --execute",
    "prune:dry": "cross-env BASE_URL=http://127.0.0.1:3000 node scripts/prune-history.js --dry",
    "prune:exec": "cross-env BASE_URL=http://127.0.0.1:3000 node scripts/prune-history.js --execute",
    "prune:prod:dry": "cross-env BASE_URL=https://www.newagefotografie.com node scripts/prune-history.js --dry",
    "prune:prod:exec": "cross-env BASE_URL=https://www.newagefotografie.com node scripts/prune-history.js --execute",
    "health:check": "echo 'Health check passed'",
    "deploy": "npm run build && npm start",
    "db:init:questionnaires": "tsx scripts/init-questionnaires.ts",
    "q:create": "node scripts/create-questionnaire.js",
    "q:submit": "node scripts/submit-questionnaire.js",
    "q:responses": "node scripts/fetch-responses.js",
    "test": "jest --env=node --runInBand",
    "test:ci": "cross-env NODE_ENV=test EMAIL_TRANSPORT=json jest --env=node --ci --runInBand"
  },
  "keywords": [
    "photography",
    "crm",
    "saas",
    "demo"
  ],
